{
  "name": "questioner-api",
  "version": "1.0.0",
  "description": "Restful api for questioner app",
  "main": "index.js",
  "scripts": {
    "db:create:dev": "createdb questionerdb_dev",
    "db:init:dev": "psql -d questionerdb_dev -f server/src/db/schema.sql",
    "db:init:test": "psql -d questionerdb_test -f server/src/db/schema.sql",
    "db:migrate": "psql -d questionerdb_dev -f server/src/db/seed.sql",
    "db:migrate:test": "psql -d questionerdb_test -f server/src/db/seed.sql",
    "build": "npm run build:server && npm run build:client",
    "build:server": "babel server/src -d server/dist",
    "build:client": "babel client/src -d client/dist",
    "start": "npm run start:server && npm run start:client:dev",
    "start:server": "npm run build:server && node server/dist/index.js",
    "start:client:dev": "export NODE_ENV=development || set NODE_ENV=development && webpack-dev-server --mode development --config webpack.dev.js --open",
    "serve": "node server/dist/index.js",
    "test:server:dev": "npm run db:init:dev && npm run db:migrate && nyc mocha server/test --require babel-register  --timeout 20000  --exit",
    "test:server": "npm run db:init:test && npm run db:migrate:test && nyc mocha --require babel-register --exit",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "deploy": "git subtree push --prefix UI origin gh-pages"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ssanusi/questioner-api.git"
  },
  "author": "Sulaiman Sanusi",
  "license": "MIT",
  "homepage": "https://ssanusi.github.io/questioner-app",
  "dependencies": {
    "babel-plugin-transform-runtime": "^6.23.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "cors": "^2.8.5",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "jsonwebtoken": "^8.4.0",
    "lodash": "^4.17.11",
    "lodash.isempty": "^4.4.0",
    "lodash.throttle": "^4.1.1",
    "moment": "^2.23.0",
    "morgan": "^1.9.1",
    "pg": "^7.8.0",
    "pg-promise": "^8.5.4",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-materialize": "^3.3.1",
    "react-modal": "^3.8.1",
    "react-redux": "^7.0.3",
    "react-router-dom": "^5.0.0",
    "react-toastify": "^5.1.1",
    "redux": "^4.0.1",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "swagger-ui-express": "^4.0.2",
    "validator": "^10.10.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^10.0.1",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "chai": "^4.2.0",
    "chai-http": "^4.2.0",
    "clean-webpack-plugin": "^2.0.2",
    "coveralls": "^3.0.2",
    "eslint": "5.3.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.1",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "7.11.0",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.7.0",
    "mocha": "^5.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^1.18.9",
    "nyc": "^14.1.1",
    "prettier": "^1.15.3",
    "webpack": "^4.32.2",
    "webpack-cli": "^3.3.2",
    "webpack-dev-middleware": "^3.7.0",
    "webpack-dev-server": "^3.4.1",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-merge": "^4.2.1"
  },
  "bugs": {
    "url": "https://github.com/ssanusi/questioner-api/issues"
  }
}
