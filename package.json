{
  "name": "questioner-api",
  "version": "1.0.0",
  "description": "Restful api for questioner app",
  "main": "index.js",
  "scripts": {
    "db:create:dev": "createdb questionerdb_dev",
    "db:create": "createdb questionerdb -U postgres",
    "db:init:dev": "psql -d questionerdb_dev -f src/db/schema.sql",
    "db:init": "psql -d questionerdb -f src/db/schema.sql",
    "db:query" : "babel-node src/db/query",
    "build": "babel src -d dist",
    "start": "npm run build && nodemon dist/index.js",
    "serve": "node dist/index.js",
    "test": "nyc mocha --require babel-register --timeout 10000 --exit",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ssanusi/questioner-api.git"
  },
  "author": "Sulaiman Sanusi",
  "license": "MIT",
  "dependencies": {
    "babel-plugin-transform-runtime": "^6.23.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "jsonwebtoken": "^8.4.0",
    "lodash": "^4.17.11",
    "lodash.isempty": "^4.4.0",
    "moment": "^2.23.0",
    "morgan": "^1.9.1",
    "pg": "^7.8.0",
    "pg-promise": "^8.5.4",
    "validator": "^10.10.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^10.0.1",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "chai": "^4.2.0",
    "chai-http": "^4.2.0",
    "coveralls": "^3.0.2",
    "eslint": "5.3.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.1",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "7.11.0",
    "mocha": "^5.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^1.18.9",
    "nyc": "^13.1.0",
    "prettier": "^1.15.3"
  },
  "bugs": {
    "url": "https://github.com/ssanusi/questioner-api/issues"
  },
  "homepage": "https://github.com/ssanusi/questioner-api#readme"
}
